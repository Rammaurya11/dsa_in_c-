    cout << "hello wold " <<'\n';   // << es oprater ka use display karane ke liye use kiya jata hay//
        cout << "hello wold " <<endl;  //( endl and '\n' ) ka use new line ke liye use kiya gaya hay//
        cout << "namste duniy \n"; //( ; ) ka use line end ke liye use kiy jata hay //

    // return 0;
}
